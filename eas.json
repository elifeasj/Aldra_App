{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENV_FILE": ".env.local"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "buildType": "release",
        "distribution": "app-store"
      },
      "autoIncrement": true,
      "env": {
        "ENV_FILE": ".env.local"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
