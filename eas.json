{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENV_FILE": ".env.local"
      }
    },
    "production": {
      "ios": {
        "enterpriseProvisioning": "off"
      },
      "env": {
        "ENV_FILE": ".env.local"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "business@aldra.dk"
      }
    }
  }
}
